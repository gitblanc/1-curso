package squares;

import board.Square;

/**
 * @author blanc
 *
 */
public class Water {
	private Square water;
	
	public int shootAt() {
		return 0;
	}
	
	public boolean hasImpact() {
		return false;
	}
	
	public char toChar() {
		return ' ';
	}
	
	public char toFiredChar() {
		return '.';
	}
	

	/**
	 * @return the water
	 */
	public Square getWater() {
		return water;
	}
}
